{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "databaseTypeORM/database-typeorm/(.*)": "<rootDir>/../libs/database-typeorm/src/$1",
    "databaseTypeORM/database-typeorm": "<rootDir>/../libs/database-typeorm/src",
    "@app/demo/demo_lib/(.*)": "<rootDir>/../libs/demo_lib/src/$1",
    "@app/demo/demo_lib": "<rootDir>/../libs/demo_lib/src",
    "@app/demo/demo/(.*)": "<rootDir>/../libs/demo/src/$1",
    "@app/demo/demo": "<rootDir>/../libs/demo/src",
    "@app/demo_lib1/(.*)": "<rootDir>/../libs/demo_lib1/src/$1",
    "@app/demo_lib1": "<rootDir>/../libs/demo_lib1/src"
  }
}